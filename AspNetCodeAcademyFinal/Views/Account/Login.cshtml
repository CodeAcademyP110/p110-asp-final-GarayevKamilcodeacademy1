@model LoginModel
    <section>
        <div class="wrapper">
            <div class="vipAll vipAdvertise">
                <b class="logoText">Bütün bank xidmətləri telefonunda</b>
                <img src="https://newmedia.az/iqbal/9/4/sag/logo2.png">

            </div>
        </div>
        <form asp-action="Login" method="post">
            <input type="hidden" name="returnUrl" value="@ViewBag.returnUrl" />
            <div class="form-group">
                <input asp-for="Email" class="au-input au-input--full" name="email" placeholder="email">
            </div>
            <div class="form-group">
                <input type="password" asp-for="Password" class="au-input au-input--full" name="password" placeholder="password">
            </div>


            <button class="au-btn au-btn--block au-btn--green m-b-20" type="submit">Login</button>
        </form>
    </section>
